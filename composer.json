{
    "autoload": {
        "psr-4": {"DocuSign\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "DocuSign\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --verbose tests"
    },
    "config": {
        "process-timeout": 0
    },
    "description": "DocuSign PHP launcher",
    "require": {
        "docusign/admin-client": "^1.4.2",
        "docusign/click-client": "^1.4.1",
        "docusign/esign-client": "^6.19.0-rc2",
        "docusign/rooms-client": "^2.1.1",
        "docusign/monitor-client": "^1.2.1",
        "docusign/webforms-client": "^1.0.1",
        "docusign/maestro-client": "^1.0.0-rc1",
        "twig/twig": "^3.5.1",
        "league/oauth2-client": "^2.6.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "7.8.1",
        "firebase/php-jwt": "6.10.0",
        "mashape/unirest-php": "3.0.4",
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "^9.5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*"
    }
}